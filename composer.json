{
    "name": "lav45/mock-server",
    "type": "library",
    "description": "HTTP mocking application for testing",
    "keywords": [
        "testing",
        "mock",
        "php",
        "http",
        "mock-server",
        "stubbing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "amphp/amp": "v3.0.*",
        "amphp/log": "v2.0.*",
        "amphp/http": "v2.1.*",
        "amphp/http-client": "v5.0.*",
        "amphp/http-server": "v3.2.*",
        "amphp/http-server-form-parser": "v2.0.*",
        "nikic/fast-route": "1.3.*",
        "monolog/monolog": "3.5.*",
        "fakerphp/faker": "1.21.*",
        "yiisoft/data": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "lav45\\MockServer\\": "src"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true
    }
}
